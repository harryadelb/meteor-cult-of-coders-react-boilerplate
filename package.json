{
  "name": "meteor-tuts",
  "private": true,
  "scripts": {
    "start": "meteor npm i && meteor run",
    "debug": "meteor run --inspect",
    
    "lint": "eslint . --ext .jsx --fix"
  },
  "meteor": {
    "mainModule": {
      "client": "client/main.jsx",
      "server": "server/main.js"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --fix",
    "*.jsx": "eslint --fix"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.2",
    "Package": "0.0.1",
    "bcrypt": "^5.0.0",
    "history": "^4.7.2",
    "meteor-node-stubs": "^0.4.1",
    "moment": "^2.20.1",
    "prop-types": "^15.6.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "simpl-schema": "^1.5.6",
    "uniforms": "^2.6.0",
    "uniforms-bridge-simple-schema-2": "^2.6.0",
    "uniforms-unstyled": "^2.6.0"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "lint-staged": "^10.1.7",
    "pre-commit": "^1.2.2"
  }
}
